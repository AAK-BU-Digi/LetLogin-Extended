{
  "name": "LetLogin Extended",
  "version": "1.1",
  "manifest_version": 2,
  "description": "LetLogin Extended XXXXXXXXXXXXX",
  
  "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://fravaer.tabulex.net/*",
        "https://saml.personale.tabulex.dk/*"
  ],

  
  "content_scripts": [
    {
      "js": [
        "forward.js"
      ],
      "matches": [
        "https://saml.personale.tabulex.dk/simplesaml/*",
        "https://broker.unilogin.dk/auth/realms/broker/*",
        "https://fravaer.tabulex.net/"
      ],
      "exclude_globs": [
              "*post-broker-login*"
              "*fravaer.tabulex.net/Home/Logud*"
      ]

    }
   
  ],
  
    "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  
    "icons": {
    "16": "Icons/LetLogin2-16.png",
    "48": "Icons/LetLogin2-48.png",
    "128": "Icons/LetLogin2-128.png"
   }
}
